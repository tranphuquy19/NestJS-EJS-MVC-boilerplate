NODE_ENV="development"
PORT="4000"
LISTEN_ON="127.0.0.1" # default binding on 127.0.0.1

TIMEZONE="Asia/Ho_Chi_Minh"

# Maintainer
EMAIL="your-email@example.com"

API_URLS="http://first-domain:4000/api/v1|https://api.second-domain.com/api/v1"
API_VERSION="api/v1"
CLIENT_URL="https://your-domain.com"

STACK_NAME="nest"
DATA_FOLDER=".docker"

# Pagination config
MAX_ITEMS_PER_PAGE="20"
ITEMS_ORDER="DESC"

# logs config
ENABLE_LOGGING="true"
LOG_DIR="logs"
ONLY_ERROR_REQUESTS="true"
LOG_FORMAT="dev"

# i18n config
DEFAULT_LOCALE="en"

# uploader config, support relative path with root directory
DEFAULT_STORAGE_DIR="./public/uploads"
DEFAULT_MAX_FILE_SIZE="1 GiB"

# session config
SESSION_SECRET="Ses5onSecret"
SESSION_MAX_AGE="1h"

# Database Config
