DATABASE_PORT=5432
DATABASE_USERNAME="postgres"
DATABASE_PASSWORD="P@s5word"
DATABASE_NAME="nest_db"
DATABASE_ENABLE_CACHE="false"
DATABASE_CACHE_DURATION="1m"

# Redis Config
